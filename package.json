{
  "name": "mre-recipes",
  "version": "1.0.0",
  "description": "More-Recipes provides a platform for users to share the awesome and exciting  recipe ideas they have invented or learnt.  Suppose a user comes up with a food recipe,  he/she can post it on More-Recipes and  get feedback in form of reviews and votes from other users who explore that recipe. Users can also keep a list of their favorite recipes on the application.",
  "main": "app.js",
  "scripts": {
    "start:dev": "NODE_ENV=development nodemon --exec babel-node -- ./server/bin/www",
    "start:test": "NODE_ENV=test sequelize db:migrate",
    "test:client": "jest",
    "test": "NODE_ENV=test mocha server/test/testPath/test.js --compilers js:babel-core/register --require babel-polyfill --reporter nyan --timeout 15000 --exit",
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "coveralls": "nyc npm test&& nyc report --reporter=text-lcov | coveralls",
    "start": "NODE_ENV=production babel-node -- ./server/bin/www",
    "build": "webpack -d  && cp client/src/index.html client/dist/index.html && webpack-dev-server --content-base ./client/src/ --inline --hot",
    "build:prod": "webpack -p  && copy src/index.html"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kensanni/More-recipes.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/kensanni/More-recipes/issues"
  },
  "homepage": "https://github.com/kensanni/More-recipes#readme",
